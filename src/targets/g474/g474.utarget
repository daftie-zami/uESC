# G474 utarget file

DEVICE = stm32g474ceu6
PLATFORM = STM32G4

USE_HSE = 1
HSE_FREQ_MHZ = 8

USE_CCM_RAM = 1

# MOTOR_PWM_TIM must be advanced control timer (TIM1, TIM8, TIM20)
MOTOR_PWM_TIM = TIM1
MOTOR_PWM_DT = 10       # Dead Time

#INPUT METHOD
USE_PWM_INPUT = 1
USE_SERIAL_INPUT = 1
USE_CAN_INPUT = 1