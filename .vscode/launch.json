{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Cortex Debug",
            "cwd": "${workspaceFolder}",
            "executable": "${workspaceFolder}/build/debug.elf",
            "request": "launch",
            "type": "cortex-debug",
            "runToEntryPoint": "main",
            "servertype": "openocd",
            "gdbPath": "gdb-multiarch",
            "configFiles": [
                "${workspaceFolder}/build/openocd.cfg"
            ],
            "svdFile": "${workspaceFolder}/build/debug.svd",
            "internalConsoleOptions": "neverOpen"
        }
    ]
}
